{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "overrides": [
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "prettier/prettier" : "error" ,
      "no-unused-vars" : "error" ,
      "spaced-comment" : "off" ,
      "no-console" : "warn" ,
      "consistent-return" :"off" ,
      "func-names" :"off" ,
      "object-shorthand" : "off" ,
      "no-process-exit" :"off" ,
      "no-param-reassign" :"off" ,
      "no-return-await" :"off" ,
      "no-underscore-dangle" :"off" ,
      "class-methods-use-this" : "off",
      "prefer-destructuring" : ["error" , {"argsIgnorePattern": "req|res|next|val"}]

        }
}
